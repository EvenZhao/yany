{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import", 
        "@babel/plugin-transform-async-to-generator",
        "@babel/plugin-proposal-function-bind",
        "transform-object-rest-spread",
        "transform-do-expressions",
        "syntax-trailing-function-commas"
    ]
}